{
  "name": "nestjs-nuxt-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/server/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env mode=production NODE_ENV=production npm run prebuild && nuxt-ts build && nest build",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development npm run prebuild && webpack --config webpack-hmr.config.js",
    "start": "cross-env NODE_ENV=production node dist/server/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest -i --config test/jest-e2e.config.js",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/platform-fastify": "^7.3.2",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "@nuxt/typescript-build": "0.6.6",
    "@nuxt/typescript-runtime": "0.4.6",
    "@nuxtjs/axios": "5.10.3",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/connect": "3.4.33",
    "@types/jest": "25.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "13.13.5",
    "@types/sharp": "^0.25.1",
    "argon2": "^0.26.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "fastify": "^2.15.2",
    "fastify-multipart": "^1.0.6",
    "fastify-plugin": "^1.6.1",
    "fastify-swagger": "^2.6.0",
    "i18next": "^19.6.3",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "2.12.2",
    "nuxt-property-decorator": "2.7.2",
    "pg": "^8.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "sharp": "^0.25.4",
    "snyk": "1.320.2",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@nuxt/types": "^2.14.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "cross-env": "7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "jest": "26.0.1",
    "node-sass": "^4.14.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "7.0.3",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.8.3",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0"
  },
  "snyk": true
}
